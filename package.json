{
  "name": "v-easy-components",
  "description": "Vue.js UI Components",
  "version": "1.2.5",
  "main": "index.js",
  "files": [
    "lib",
    "src",
    "types",
    "packages"
  ],
  "keywords": [
    "Vue",
    "UI",
    "Components"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Linkontoask/v-easy-components.git"
  },
  "scripts": {
    "bootstrap": "yarn || npm i",
    "dev": "webpack-dev-server --config config/webpack-dev-config.js",
    "dev:docs": "vuepress dev docs",
    "build": "npm run clean && npm run lint && npm run build:main && npm run build:cm && npm run build:common && npm run build:theme",
    "build:main": "webpack --config config/webpack-build-config.js",
    "build:cm": "webpack --config config/webpack-build-components.js",
    "build:common": "webpack --config config/webpack-build-common.js",
    "build:theme": "rimraf src/theme-chalk/lib && gulp build --gulpfile src/theme-chalk/gulpfile.js && rimraf src/theme-chalk/lib/_font.css && cp-cli src/theme-chalk/lib lib/theme-chalk && rimraf lib/theme-chalk/lib",
    "build:docs": "vuepress build docs",
    "lint": "eslint \"packages/**/*\" \"src/**/*.js\" --quiet",
    "test": "npm run lint && cross-env CI_ENV=/dev/ BABEL_ENV=test karma start test/karma.conf.js --single-run",
    "test:watch": "npm run lint && cross-env BABEL_ENV=test karma start test/karma.conf.js",
    "clean": "rimraf lib",
    "validate:commit": "validate-commit-msg",
    "change:log": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "prettier": "prettier --write packages/**/*.vue packages/**/*.js example/**"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run validate:commit && npm run lint"
    }
  },
  "homepage": "https://linkorg.club",
  "bugs": {
    "url": "https://github.com/Linkontoask/v-easy-components/issues"
  },
  "style": "lib/theme-chalk/index.css",
  "typings": "types/index.d.ts",
  "unpkg": "lib/index.min.js",
  "author": "Link",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@vuepress/plugin-back-to-top": "^1.2.0",
    "@vuepress/plugin-google-analytics": "^1.2.0",
    "@vuepress/plugin-pwa": "^1.2.0",
    "acorn": "^6.3.0",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-istanbul": "^5.2.0",
    "babel-regenerator-runtime": "^6.5.0",
    "chai": "^4.2.0",
    "conventional-changelog-cli": "^2.0.25",
    "cp-cli": "^2.0.0",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^6.0.1",
    "file-loader": "^1.1.11",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "gulp": "^4.0.2",
    "gulp-cssmin": "^0.2.0",
    "gulp-less": "^4.0.1",
    "gulp-postcss": "^8.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.1",
    "karma-mocha": "^1.3.0",
    "karma-sinon-chai": "^2.0.2",
    "karma-spec-reporter": "^0.0.32",
    "karma-webpack": "^4.0.2",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "mocha": "^6.2.2",
    "node-sass": "^4.12.0",
    "path": "^0.12.7",
    "postcss": "^5.2.18",
    "postcss-loader": "^3.0.0",
    "postcss-salad": "^2.0.1",
    "prettier": "^1.19.1",
    "progress-bar-webpack-plugin": "^1.12.1",
    "rimraf": "^3.0.0",
    "sass-loader": "^7.2.0",
    "sinon": "^7.5.0",
    "sinon-chai": "^3.3.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "2.2.2",
    "ts-loader": "^6.0.3",
    "typescript": "^3.5.2",
    "url-loader": "^2.0.0",
    "validate-commit-msg": "^2.14.0",
    "vue": "^2.6.10",
    "vue-i18n": "^8.7.0",
    "vue-loader": "^15.7.0",
    "vue-router": "^3.1.3",
    "vue-template-compiler": "^2.6.10",
    "vuepress": "1.2.0",
    "webpack": "^4.34.0",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.1.0",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "deepmerge": "^3.1.0",
    "popper.js": "^1.16.0",
    "regenerator-runtime": "^0.13.3"
  },
  "peerDependencies": {
    "vue": "^2.6.10"
  }
}
